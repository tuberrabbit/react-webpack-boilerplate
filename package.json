{
  "name": "my-react-webpack-boilerplate",
  "version": "0.0.1",
  "description": "react-webpack-boilerplate",
  "scripts": {
    "start": "TS_NODE_PROJECT=\"tsconfig-for-webpack-config.json\" webpack-dev-server --config webpack.config.dev.ts --open",
    "build": "TS_NODE_PROJECT=\"tsconfig-for-webpack-config.json\" webpack",
    "test": "jest"
  },
  "keywords": [
    "react",
    "webpack"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/tuberrabbit/react-webpack-boilerplate.git"
  },
  "author": "Tuber",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "21.1.8",
    "@types/node": "8.0.53",
    "@types/react": "16.0.25",
    "@types/webpack": "3.8.1",
    "@types/webpack-env": "1.13.5",
    "awesome-typescript-loader": "3.4.0",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-plugin-lodash": "3.3.2",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-3": "6.24.1",
    "clean-webpack-plugin": "0.1.17",
    "css-loader": "0.28.7",
    "ejs-loader": "0.3.0",
    "enzyme": "3.2.0",
    "enzyme-adapter-react-16": "1.1.1",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "html-webpack-plugin": "2.30.1",
    "jest": "21.2.1",
    "less": "2.7.3",
    "less-loader": "4.0.5",
    "lodash-webpack-plugin": "0.11.4",
    "optimize-css-assets-webpack-plugin": "3.2.0",
    "path": "0.12.7",
    "style-loader": "0.19.0",
    "ts-jest": "21.2.4",
    "ts-node": "3.3.0",
    "tsconfig-paths": "3.1.1",
    "tslint": "5.8.0",
    "tslint-eslint-rules": "4.1.1",
    "tslint-react": "3.2.0",
    "typescript": "2.6.2",
    "uglifyjs-webpack-plugin": "1.1.1",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.5"
  },
  "dependencies": {
    "babel-runtime": "6.26.0",
    "dva": "2.1.0",
    "dva-loading": "1.0.4",
    "history": "4.7.2",
    "lodash": "4.17.4",
    "normalize.css": "7.0.0",
    "promise-polyfill": "6.1.0",
    "raf": "3.4.0",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-loadable": "5.3.1"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "bail": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/components/**/*.{ts,tsx}",
      "**/containers/**/*.{ts,tsx}"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "dist_ts",
      "dist"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupTest.ts"
  }
}
